{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "rules": {
    "comma-dangle": "off",
    "no-console": "off",
    // "react-hooks/exhaustive-deps": 0,
    /*"prettier/prettier": [
      "error"
    ]*/
    "prettier/prettier": 0
  }
}
